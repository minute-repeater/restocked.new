{
  "name": "stockcheck-fetcher",
  "version": "1.0.0",
  "description": "Page fetching module with HTTP and Playwright fallback",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc && tsc scripts/telegram.ts --outDir dist/scripts --module Node16 --moduleResolution node16 --target es2020 --esModuleInterop --skipLibCheck",
    "start": "node dist/db/runMigrationsAndStart.js",
    "start:server-only": "node dist/api/server.js",
    "start:worker": "node dist/worker/scheduler.js",
    "start:all": "npm run start:server-only & npm run start:worker",
    "dev": "tsc --watch",
    "dev:server": "tsc --watch & node --watch dist/api/server.js",
    "dev:worker": "tsc && node --watch dist/worker/scheduler.js",
    "test": "node --loader ts-node/esm tests/fetcher.test.ts",
    "profile:extractor": "NODE_OPTIONS='--max-old-space-size=8192 --expose-gc --loader ts-node/esm' node scripts/profileExtractor.ts",
    "test:monitoring": "NODE_OPTIONS='--loader ts-node/esm' node scripts/test-monitoring.ts",
    "migrate": "node ./dist/db/migrate.js",
    "verify-migrations": "NODE_OPTIONS='--loader ts-node/esm' node scripts/verify-migrations.ts",
    "seed:prod-user": "NODE_OPTIONS='--loader ts-node/esm' node scripts/seed-prod-user.ts",
    "create-test-user": "NODE_OPTIONS='--loader ts-node/esm' node scripts/create-test-user.ts",
    "test:telegram": "node dist/scripts/telegram.js"
  },
  "keywords": [
    "scraping",
    "playwright",
    "http",
    "fetch"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^10.29.0",
    "@sentry/tracing": "^7.120.4",
    "@types/cors": "^2.8.19",
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "googleapis": "^167.0.0",
    "jsonwebtoken": "^9.0.3",
    "jwks-rsa": "^3.2.0",
    "mongodb": "^7.0.0",
    "node-cron": "^4.2.1",
    "pg": "^8.11.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "playwright": "^1.40.0",
    "react-intersection-observer": "^10.0.0",
    "resend": "^6.5.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.9",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  }
}
